  if (!Array.isArray(data.–ü—Ä–æ–≤–∏–Ω—Ü–∏–∏)) {
    data.–ü—Ä–æ–≤–∏–Ω—Ü–∏–∏ = [];
  }

  let –¥–æ–±–∞–≤–ª–µ–Ω–æ = 0;

  // –ü—Ä–æ—Ö–æ–¥–∏–º –ø–æ –í–°–ï–ú —è—á–µ–π–∫–∞–º (–æ—Ç 0 –¥–æ —Ç–µ–∫—É—â–µ–π –¥–ª–∏–Ω—ã –º–∞—Å—Å–∏–≤–∞)
  for (let i = 0; i < data.–ü—Ä–æ–≤–∏–Ω—Ü–∏–∏.length; i++) {
    // –ï—Å–ª–∏ —è—á–µ–π–∫–∞ –Ω–µ –º–∞—Å—Å–∏–≤ ‚Äî –¥–µ–ª–∞–µ–º –µ—ë –º–∞—Å—Å–∏–≤–æ–º
    if (!Array.isArray(data.–ü—Ä–æ–≤–∏–Ω—Ü–∏–∏[i])) {
      data.–ü—Ä–æ–≤–∏–Ω—Ü–∏–∏[i] = [];
    }

    // –ï—Å–ª–∏ —è—á–µ–π–∫–∞ –ø—É—Å—Ç–∞—è ‚Äî –¥–æ–±–∞–≤–ª—è–µ–º –¥–µ—Ñ–æ–ª—Ç–Ω—É—é –ø—Ä–æ–≤–∏–Ω—Ü–∏—é
    if (data.–ü—Ä–æ–≤–∏–Ω—Ü–∏–∏[i].length === 0) {
      const –Ω–æ–º–µ—Ä–Ø—á–µ–π–∫–∏ = i + 1;

      const –¥–µ—Ñ–æ–ª—Ç–Ω–∞—è–ü—Ä–æ–≤–∏–Ω—Ü–∏—è = {
        "–ü—Ä–æ–≤–∏–Ω—Ü–∏—è": `–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –ø—Ä–æ–≤–∏–Ω—Ü–∏—è ${–Ω–æ–º–µ—Ä–Ø—á–µ–π–∫–∏}`,
        "–ö–ª–∏–º–∞—Ç": ["–£–º–µ—Ä–µ–Ω–Ω—ã–π –∫–æ–Ω—Ç–∏–Ω–µ–Ω—Ç–∞–ª—å–Ω—ã–π"],
        "–õ–∞–Ω–¥—à–∞—Ñ—Ç": ["–†–∞–≤–Ω–∏–Ω—ã"],
        "–í–ª–∞–¥–µ–ª–µ—Ü": "–°–≤–æ–±–æ–¥–Ω—ã–µ –∑–µ–º–ª–∏",
        "–ü–ª–æ—â–∞–¥—å": 1000,
        "–ê–≥—Ä–∞—Ä–Ω—ã–µ –∑–µ–º–ª–∏": 800,
        "–ü–ª–∞–Ω–µ—Ç–∞": ["–ó–µ–º–ª—è"],
        "–≠–∫–æ–ª–æ–≥–∏—è": [
          {
            "–†–∞–¥–∏–æ–∞–∫—Ç–∏–≤–Ω–æ–µ –∑–∞–≥—Ä—è–∑–Ω–µ–Ω–∏–µ": 0,
            "–ó–∞–≥—Ä—è–∑–Ω–µ–Ω–∏–µ": 0
          }
        ],
        "–†–µ—Å—É—Ä—Å—ã": [
          {
            "–î—Ä–µ–≤–µ—Å–∏–Ω–∞": 1000,
            "–ü–∏—Ç—å–µ–≤–∞—è –≤–æ–¥–∞": 100000
          }
        ]
      };

      data.–ü—Ä–æ–≤–∏–Ω—Ü–∏–∏[i].push(–¥–µ—Ñ–æ–ª—Ç–Ω–∞—è–ü—Ä–æ–≤–∏–Ω—Ü–∏—è);
      –¥–æ–±–∞–≤–ª–µ–Ω–æ++;
      console.log(`‚úÖ –í —è—á–µ–π–∫—É \( {–Ω–æ–º–µ—Ä–Ø—á–µ–π–∫–∏} –¥–æ–±–∞–≤–ª–µ–Ω–∞ –¥–µ—Ñ–æ–ª—Ç–Ω–∞—è –ø—Ä–æ–≤–∏–Ω—Ü–∏—è: \){–¥–µ—Ñ–æ–ª—Ç–Ω–∞—è–ü—Ä–æ–≤–∏–Ω—Ü–∏—è.–ü—Ä–æ–≤–∏–Ω—Ü–∏—è}`);
    }
  }

  if (–¥–æ–±–∞–≤–ª–µ–Ω–æ > 0) {
    data.–ù–æ–≤–æ—Å—Ç–∏.push(`–ó–∞–ø–æ–ª–Ω–µ–Ω–æ \( {–¥–æ–±–∞–≤–ª–µ–Ω–æ} –ø—É—Å—Ç—ã—Ö —è—á–µ–µ–∫ –¥–µ—Ñ–æ–ª—Ç–Ω—ã–º–∏ –ø—Ä–æ–≤–∏–Ω—Ü–∏—è–º–∏. –í—Å–µ–≥–æ —è—á–µ–µ–∫: \){data.–ü—Ä–æ–≤–∏–Ω—Ü–∏–∏.length}`);
