function generateDefaultProvinces(data) {
  console.log('üåç –ü—Ä–æ–≤–µ—Ä–∫–∞ –≤—Å–µ—Ö —è—á–µ–µ–∫ –≤ data.–ü—Ä–æ–≤–∏–Ω—Ü–∏–∏ –∏ –∑–∞–ø–æ–ª–Ω–µ–Ω–∏–µ –ø—É—Å—Ç—ã—Ö –¥–µ—Ñ–æ–ª—Ç–Ω—ã–º–∏ –ø—Ä–æ–≤–∏–Ω—Ü–∏—è–º–∏');

